{
    "name": "iamthesaifullah/surge",
    "description": "A Laravel package for asynchronous task dispatching and worker management.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Md Saifullah",
            "email": "imthesaifullah@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0 || ^11.0",
        "illuminate/console": "^10.0 || ^11.0",
        "illuminate/queue": "^10.0 || ^11.0",
        "illuminate/bus": "^10.0 || ^11.0",
        "symfony/process": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Surge\\\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Surge\\\\SurgeServiceProvider"
            ],
            "aliases": {
                "Surge": "Surge\\\\Facades\\\\Surge"
            }
        }
    }
}
